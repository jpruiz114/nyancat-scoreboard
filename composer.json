{
    "name": "whatthejeff/nyancat-scoreboard",
    "description": "Nyan Cat Scoreboard",
    "keywords": ["nyan","cat","rainbow","scoreboard","tests"],
    "homepage": "http://github.com/whatthejeff/nyancat-scoreboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Welch",
            "email": "whatthejeff@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "whatthejeff/fab": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*@dev",
        "phpunit/php-code-coverage": "2.0.*@dev",
        "phpunit/phpunit-mock-objects": "2.0.*@dev",
        "pear-pear/pear": "1.9.4"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "NyanCat": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
